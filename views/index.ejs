                    <img src="imgs/lupa.svg" alt="" width="23" height="23">
                    <input type="text" class="InputInside p-3" placeholder="Search for books" id="input2search">
                </label>
                <label class="serachButton">
                    <button class="serachBtn rainbow-btn" onclick="search2input()">search</button>
                </label>
            </div>
            <label class="buyCart">
                <!-- Button trigger modal -->
                <button type="button" class="btn btn-primary buyCartButton" data-bs-toggle="modal"
                    data-bs-target="#exampleModal">
                    <img src="imgs/cart.svg" alt="" width="36" height="36">
                </button>

                <!-- Modal -->
                <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel"
                    aria-hidden="true">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h1 class="modal-title fs-5" id="exampleModalLabel">Your Loans</h1>
                                <button type="button" class="btn-close" data-bs-dismiss="modal"
                                    aria-label="Close"></button>
                            </div>
                            <div class="modal-body">
                                <% borrow.forEach(function(livro) { %>
                                    <div>
                                        <p>
                                            <%= livro.name %>
                                        </p>
                                        <p>
                                            <%= livro.Data_Emprestimo.toLocaleDateString('pt-BR') %>
                                        </p>
                                        <p>
                                            <%= livro.Data_Devolucao.toLocaleDateString('pt-BR') %>
                                        </p>
                                    </div>
                                    <% }) %>
                            </div>
                        </div>
